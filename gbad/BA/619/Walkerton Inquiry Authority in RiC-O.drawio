        <mxCell id="_-RjwaxhbeTWfwVWe4ke-14" value="rico:history" style="edgeLabel;resizable=0;html=1;;align=center;verticalAlign=middle;" parent="_-RjwaxhbeTWfwVWe4ke-13" connectable="0" vertex="1">
