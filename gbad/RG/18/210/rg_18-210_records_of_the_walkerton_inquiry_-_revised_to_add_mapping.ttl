                                                                                                  <http://gbad.archives.gov.on.ca/RG%2018-210> ;
                                                                          rico:hasOrHadInstantiation <http://gbad.archives.gov.on.ca/Records%20Of%20The%20Walkerton%20Inquiry-i1> ;
